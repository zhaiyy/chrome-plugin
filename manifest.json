{
  "name": "paopaoTool",
  "version": "1.1",
  "manifest_version": 2,
  "description":
    "泡泡h5开发工具",
  "icons": {
    "16": "static/images.png",
    "48": "static/images.png",
    "128": "static/images.png"
  },
  "browser_action": {
    "default_title": "paopaoTool",
    "default_icon": "static/images.png",
    "default_popup": "index.html"
  },
  "omnibox": {
    "keyword": "metools"
  },
  "offline_enabled": true,
  "permissions": ["tabs"]
}
